{
  "name": "webpack-env-js",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "webpack-dev-server --config config/webpack.client.dev.js",
    "bundle:dev": "env-cmd -f .env.development webpack --config config/webpack.client.dev.js",
    "bundle:prod": "env-cmd -f .env.production webpack --config config/webpack.client.prod.js",
    "bundle:ssr": "env-cmd -f .env.development webpack --config config/webpack.server.dev.js",
    "server:dev": "env-cmd -f .env.development nodemon server/main.js",
    "server:prod": "env-cmd -f .env.production nodemon server/main.js",
    "server:ssr:dev": "env-cmd -f .env.development nodemon server/main-ssr.js",
    "server:ssr:prod": "env-cmd -f .env.production nodemon server/main-ssr.js",
    "debug": "nodemon --inspect server/main.js",
    "debug:ssr": "nodemon --inspect server/main-ssr.js",
    "http": "lite-server ./dist",
    "eslint": "eslint ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "express-static-gzip": "^2.0.7",
    "lodash": "^4.17.20",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-universal-component": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@hot-loader/react-dom": "^16.13.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-file-loader": "^2.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^5.0.1",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.2.1",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "extract-css-chunks-webpack-plugin": "^4.7.5",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "lite-server": "^2.5.4",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^0.10.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
