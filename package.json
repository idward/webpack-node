    "webpack-merge": "^5.1.2",
    "webpack-mild-compile": "^3.3.1",
    "webpack-node-externals": "^2.5.2"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "IE 10"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run eslint"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --list-different"
    ]
  }
}
